<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.a_nil.aarogya" >

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE"/>

    <application
        android:allowBackup="true"
        android:icon="@drawable/app_iconx72"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="com.example.a_nil.aarogya.MainActivity"
            android:label="@string/app_name"
            android:theme="@style/AppTheme">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.example.a_nil.aarogya.Register"
            android:label="@string/title_activity_rigister" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.example.a_nil.aarogya.MainActivity" />
        </activity>
        <activity
            android:name="com.example.a_nil.aarogya.Welcome"
            android:label="@string/app_name" >
        </activity>
        <activity
            android:name="com.example.a_nil.aarogya.Healthrecorder"
            android:label="@string/title_activity_healthrecorder" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.example.a_nil.aarogya.Welcome" />
        </activity>
        <activity
            android:name="com.example.a_nil.aarogya.Recorder_View"
            android:label="@string/title_activity_recorder__view"
            android:parentActivityName="com.example.a_nil.aarogya.Healthrecorder" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.example.a_nil.aarogya.Healthrecorder" />
        </activity>
        <activity
            android:name="com.example.a_nil.aarogya.Add_record"
            android:label="@string/title_activity_add_record"
            android:parentActivityName="com.example.a_nil.aarogya.Healthrecorder" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.example.a_nil.aarogya.Healthrecorder" />
        </activity>
        <activity
            android:name="com.example.a_nil.aarogya.Healthbuzz"
            android:label="@string/title_activity_newsbuzz"
            android:parentActivityName="com.example.a_nil.aarogya.Welcome" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.example.a_nil.aarogya.Welcome" />
        </activity>
        <activity
            android:name="com.example.a_nil.aarogya.NewsDetails"
            android:label="@string/title_activity_newsdetail"
            android:parentActivityName="com.example.a_nil.aarogya.Healthbuzz" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.example.a_nil.aarogya.Healthbuzz" />
        </activity>
        <activity
            android:name="com.example.a_nil.aarogya.CalorieCalculator"
            android:label="@string/title_activity_calorie_calculator"
            android:parentActivityName="com.example.a_nil.aarogya.Welcome"
            android:theme="@style/AppTheme.NoActionBar" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.example.a_nil.aarogya.Welcome" />
        </activity>
    </application>

</manifest>
